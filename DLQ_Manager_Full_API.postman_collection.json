					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Save the DLQ ID for later use",
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    pm.collectionVariables.set('dlqTopicId', jsonData.dlqTopic.id);",
									"    console.log('Saved DLQ ID:', jsonData.dlqTopic.id);",
									"}"
								],
